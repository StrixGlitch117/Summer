#include <iosteam>
using namespace std;

int main(){
  int i,j;
  for (;;){
   cout<<i<<" ";
   if (i % 5 == 0)cout<<endl;
   i = i + 1;
   j++;
   if (j > 1000000) break;
  }
  return 0;



